{
  "expo": {
    "name": "Ordering App",
    "slug": "WebAppWrapperExpo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/pscLogo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/pscLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.psckiosk.WebAppWrapperExpo",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      }
    },
    "android": {
      "package": "com.psckiosk.WebAppWrapperExpo",
      "label": "Ordering App",
      "usesCleartextTraffic": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/pscLogo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/pscLogo.png"
    },
    "extra": {
      "eas": {
        "projectId": "5c7de5d4-a96b-4a4c-86d7-c09dc4af9a4a"
      }
    },
    "owner": "partnersolutions"
  }
}
