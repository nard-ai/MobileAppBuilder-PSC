# Environment Configuration for Mobile App Builder Server
# Copy this file to .env and fill in your values

# Required: Your Expo Access Token
# Get this from: https://expo.dev/accounts/[your-account]/settings/access-tokens
EXPO_TOKEN=your_expo_token_here

# Optional: Server Configuration
NODE_ENV=production
API_PORT=3000

# Optional: Build Settings
DEFAULT_APP_NAME=My POS App
DEFAULT_BUILD_PROFILE=production

# Optional: ngrok Configuration (if using ngrok)
# NGROK_AUTH_TOKEN=your_ngrok_token_here